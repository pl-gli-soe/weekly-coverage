Public Sub create_PRIO_pivot(e As pivot_layout)
    Dim ph As PivotHandler
    Set ph = New PivotHandler
    
    Application.EnableEvents = False
    
    ph.if_flat_table_prepare_source_range
    ph.init e
    ph.config_pivot
    ph.add_slicers e
    
    Application.EnableEvents = True
End Sub



Public Sub create_PRIO_pivot_EDACW(ictrl As IRibbonControl)

    create_PRIO_pivot EDACW

End Sub


Public Sub create_PRIO_pivot_EDA(ictrl As IRibbonControl)

    create_PRIO_pivot EDA

End Sub

